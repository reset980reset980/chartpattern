# Gemini AI API Key
# Get your API key from: https://makersuite.google.com/app/apikey
API_KEY=your_gemini_api_key_here

# Google OAuth Configuration
# Get credentials from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3006/api/auth/google/callback

# Kakao OAuth Configuration
# Get credentials from: https://developers.kakao.com/
KAKAO_CLIENT_ID=your_kakao_client_id_here
KAKAO_REDIRECT_URI=http://localhost:3006/api/auth/kakao/callback

# Naver OAuth Configuration
# Get credentials from: https://developers.naver.com/apps/
NAVER_CLIENT_ID=your_naver_client_id_here
NAVER_CLIENT_SECRET=your_naver_client_secret_here
NAVER_REDIRECT_URI=http://localhost:3006/api/auth/naver/callback

# Client Application URL
# URL where your frontend is running
CLIENT_URL=http://localhost:3005
